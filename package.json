    "mouse-control",
    "keyboard-control"
  ],
  "author": "jasondsmith72",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasondsmith72/total-pc-control/issues"
  },
  "homepage": "https://github.com/jasondsmith72/total-pc-control#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.3.1",
    "@nut-tree-fork/nut-js": "^4.2.6",
    "@types/sharp": "^0.31.1",
    "sharp": "^0.33.5",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
